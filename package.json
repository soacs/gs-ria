  "scripts": {
    "ng": "nx",
    "postinstall": "ngcc",
    "nx": "nx",
    "postnx": "node checkAndClearCache.js",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "pre-commit:windows": "(nx run-many --all --target=lint --parallel & nx run-many --all --target=test --code-coverage --parallel) && exit",
    "pre-commit:mac": "nx run-many --all --target=lint --parallel && nx run-many --all --target=test --code-coverage --parallel",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "build:ui-allocation": "nx build --prod --project ui-allocation --output-hashing none && type  .\\dist\\elements\\ui-allocation\\runtime.js .\\dist\\elements\\ui-allocation\\polyfills.js .\\dist\\elements\\ui-allocation\\main.js > .\\dist\\elements\\ui-allocation\\ui-allocation.js",
    "deploy:ui-allocation": "aws s3 cp .\\dist\\elements\\ui-allocation\\ui-allocation.js s3://ria-elements/ui-allocation/ui-allocation.js --profile=sbx",
    "deploy:ui-integration": "aws s3 cp .\\dist\\elements\\ui-integration\\ui-integration.js s3://ria-elements/ui-integration/ui-integration.js --profile=sbx",
    "workspace-generator": "nx workspace-generator",
    "postinstall:bak": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only",
    "build:ui-integration": "nx build --prod --project ui-integration --output-hashing none && type  .\\dist\\elements\\ui-integration\\runtime.js .\\dist\\elements\\ui-integration\\polyfills.js .\\dist\\elements\\ui-integration\\main.js > .\\dist\\elements\\ui-integration\\ui-integration.js",
    "build:investing-elements": "nx build --prod --project investing-elements --output-hashing none && cat ./dist/elements/investing-elements/{runtime,polyfills,main}.js > ./dist/elements/investing-elements/investing-elements.js && cp ./apps/investing-elements/package.json ./dist/elements/investing-elements/",
    "deploy:preview": "npm run build:trading && aws s3 cp ./dist/apps/trading s3://ria-preview --recursive && npm run clearCloudFrontCache:preview",
    "deploy:published": "ng build --aot --prod && aws s3 cp ./dist s3://ria-published --recursive",
    "build:trading": "ng build trading --prod --output-hashing none",
    "deploy:templates": "cp -r ./templates/ ./dist/apps/trading",
    "deploy:trading": "npm run build:trading && npm run deploy:templates && aws s3 cp ./dist/apps/trading s3://ria-trading-sbx --recursive --exclude 'favicon.ico' --exclude 'index.html' --exclude '3rdpartylicenses.txt'",
    "clearCloudFrontCache:ria-elements": "aws cloudfront create-invalidation --distribution-id E2HJ1FA52KHFPD --paths \"/*\"",
    "clearCloudFrontCache:preview": "aws cloudfront create-invalidation --distribution-id E3K1ZVWBJUTB9P --paths '/*'",
    "clearCloudFrontCache:publish": "aws cloudfront create-invalidation --distribution-id E1CGW1P64YDM9C --paths '/*'",
    "clearCloudFrontCache:trading": "aws cloudfront create-invalidation --distribution-id E207VHUP59Q7MK --paths '/*'",
    "build-pack": "node ./buildpack.js"
  },
  "private": true,
